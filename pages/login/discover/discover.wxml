      <view class="arrow"></view>
    </view>
  </view>

  <!-- 看一看、搜一搜 -->
  <view class="twolists">
    <!-- 看一看 -->
    <view class="shouCang">
      <image src="../../../image/discover/kanyikan.png" class="collect" />
      <view class="shoucangText">看一看</view>
      <view class="arrow"></view>
    </view>

    <!-- 搜一搜 -->
    <view class="shouCang">
      <image src="../../../image/discover/souyisou.png" class="collect" />
      <view class="shoucangText">搜一搜</view>
      <view class="arrow"></view>
    </view>
  </view>

  <!-- 附近 -->
  <view class="onelist">
    <view class="imglist">
      <image src="../../../image/discover/fujin.png" class="serve" />
    </view>
    <view class="oneText">附近</view>
    <view class="arrow"></view>
  </view>

    <!-- 购物、游戏 -->
  <view class="twolists">
    <!-- 购物 -->
    <view class="shouCang">
      <image src="../../../image/discover/gouwu.png" class="collect" />
      <view class="shoucangText">购物</view>
      <view class="arrow"></view>
    </view>

    <!-- 游戏 -->
    <view class="shouCang">
      <image src="../../../image/discover/youxi.png" class="collect" />
      <view class="shoucangText">游戏</view>
      <view class="arrow"></view>
    </view>
  </view>
    
  
  
  <!-- 小程序 -->
  <view class="onelist">
    <view class="imglist">
      <image src="../../../image/discover/xiaochengxv.png" class="serve" />
