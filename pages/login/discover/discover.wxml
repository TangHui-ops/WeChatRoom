<view class="container">

  <!-- 朋友圈 -->
  <view class="onelist">
    <view class="imglist">
      <image src="../../../image/discover/pengyouquan.png" class="serve" />
    </view>
    <view class="oneText">朋友圈</view>
    <view class="arrow"></view>
  </view>
 
  <!-- 视频号、直播 -->
  <view class="twolists">
    <!-- 视频号 -->
    <view class="shouCang">
      <image src="../../../image/discover/shipinhao.png" class="collect" />
      <view class="shoucangText">视频号</view>
      <view class="arrow"></view>
    </view>

    <!-- 直播 -->
    <view class="shouCang">
      <image src="../../../image/discover/zhibo.png" class="collect" />
      <view class="shoucangText">直播</view>
      <view class="arrow"></view>
    </view>
  </view>
     
  <!-- 扫一扫、摇一摇 -->
  <view class="twolists">
    <!-- 扫一扫 -->
    <view class="shouCang">
      <image src="../../../image/discover/saoyisao.png" class="collect" />
      <view class="shoucangText">扫一扫</view>
      <view class="arrow"></view>
    </view>

    <!-- 摇一摇 -->
    <view class="shouCang">
      <image src="../../../image/discover/yaoyiyao.png" class="collect" />
